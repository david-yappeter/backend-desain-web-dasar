type PostLike {
    id: ID!
    created_at: String!
    user_id: ID!
    post_id: ID!
    user: User! @goField(forceResolver: true)
}

input NewPostLike {
    post_id: ID!
}

type PostLikeOps {
    create(input: NewPostLike!): PostLike! @goField(forceResolver: true) @isLogin
    delete(id: ID!): String! @goField(forceResolver: true) @isLogin
}